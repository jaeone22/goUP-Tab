{
  "manifest_version": 3,
  "name": "goUP Tab",
  "version": "1.1",
  "description": "goUP Tab을 시작페이지로 설정해요",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup/index.html"
  },
  "chrome_url_overrides": {
    "newtab": "page/index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tab@goup.im"
    }
  },
  "icons": {
    "128": "icon/128.png"
  }
}
